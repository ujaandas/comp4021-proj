  "main": "src/index.ts",
  "scripts": {
    "cp-public": "mkdir -p dist/public && cp -r public/{index.html,lobby.html,game.html,css,js} dist/public",
    "cp-server": "mkdir -p dist/server && cp -r server dist",
    "build": "tsc -p tsconfig.json && npm run cp-public && npm run cp-server",
    "clean": "rm -rf dist",
    "start": "node --watch dist/server/index.js",
    "compile": "tsc --watch",
    "dev": "npm run build && npm run compile & npm run start"
  },
